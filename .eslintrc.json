{
  "parserOptions": {
    "ecmaFeatures": {
      "impliedStrict": true
    }
  },
  "env": {
    "browser": true,
    "jquery": true,
    "amd": true
  },
  "extends": ["eslint:recommended", "plugin:lodash/v3", "prettier"],
  "globals": {
    "api": "readonly",
    "model": "writable",
    "_": "readonly",
    "loc": "readonly",
    "ko": "readonly",
    "requireGW": "readonly",
    "star_system_templates": "readonly",
    "parse": "readonly",
    "loadScript": "readonly",
    "locUpdateDocument": "readonly",
    "createjs": "readonly",
    "CommanderUtility": "readonly",
    "CardViewModel": "readonly",
    "engine": "readonly",
    "loadMods": "readonly",
    "scene_mod_list": "readonly",
    "app": "readonly"
  },
  "plugins": ["lodash"],
  "rules": {
    "lodash/preferred-alias": [2, { "ignoreMethods": ["extend", "any"] }],
    "lodash/prefer-lodash-method": 0
  }
}
